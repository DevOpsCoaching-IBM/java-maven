                    echo 'done'
      
                 }
             }
         }
     stage('first'){
         steps{
             script {
                logs.info 'Starting'
                logs.warning 'Nothing to do!'
                sayHello()
                sayHello
                sayHello('sdf')
        
                
             }
         }
     }
 }

}
